.texte_refresh_liste_ba {
	display: flex;
	position: absolute;
	top: -30px;
    right: 0;
	justify-content: flex-end;
	cursor: pointer;
}

.texte_refresh_liste_ba>div:first-of-type {
	font-size: 12px;
	
}


.refresh_liste_ba {
    position: relative;
    height: 15px;
    width: 15px;
    transform: rotate(45deg);
	margin-left: 5px;
}


.refresh_liste_ba span.horizontal {
	position: absolute;
	top: 44%;
	left: 0;
	right: 0;
    background-color: red;
	content: "";
	height: 2px;
}

.refresh_liste_ba span.vertical {
    position: absolute;
    top: 0;
    left: 44%;
	bottom:0;
	width: 2px;
    background-color: red;
    content: "";
}



