/* CSS commun à tous les tableaux de type listing */
.formulaire_liste table.tableau_listing {
  border-collapse: collapse;
  border-spacing: 0;
}
.formulaire_liste table.tableau_listing thead, .formulaire_liste table.tableau_listing tbody tr {
  display: table;
  table-layout: fixed;
  height: 28px;
}
.formulaire_liste table.tableau_listing thead th {
  text-align: left;
  padding-left: 10px;
  padding-right: 10px;
}
.formulaire_liste table.tableau_listing tbody {
  display: block;
  color: white;
  max-height: 500px;
  overflow: scroll;
}
.formulaire_liste table.tableau_listing tbody td {
  text-align: left;
  padding-left: 10px;
  padding-right: 10px;
  font-size: 14px;
  letter-spacing: -1px;
  border: none;
}
.formulaire_liste table.tableau_listing tbody tr:nth-child(even) {
  background-color: #252b33;
}
.formulaire_liste table.tableau_listing tbody tr:nth-child(odd) {
  background-color: #240909;
}
