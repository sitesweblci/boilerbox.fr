/* ********************* Boutons switch ************************** */
.switch {
	position: relative;
	display: inline-block;
	width: 50px;
	height: 21px;
}
.switch input {
	display: none;
}
.slider {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: #2196F3;
  	cursor: pointer;
	border-radius: 30px;
}

.slider::before {
	position: absolute;
	width: 15px;
	height: 15px;
	border-radius: 50%;
	background-color: #ededed;
	left: 3px;
	bottom: 3px;
	content: "";
	transition: .5s;
	transition-timing-function: ease-out;
}



input:checked + .slider::before {
	transform: translateX(30px);
  	-webkit-transform: translateX(30px);
  	-ms-transform: translateX(30px);
}


#info_bulle_fichier {
	position: absolute; 	
	top: 0; 
	right: 5px; 
	font-size: 10px; 
	height: 10px;
	text-transform: none;
	background-color: #ccc;
	border-radius: 30px;
	padding: 3px;
	color: black;
}

#info_bulle_fichier.fichier::before {
	content: 'Ajouter des fichiers';
}

#info_bulle_fichier::before {
    content: 'Afficher les commentaires';
}

